<header>
  <h1>Offices</h1>
  
  <ul class="actions">
    <li>
      <%= link_to "New Office", new_office_path, class: "new" %>
    </li>
  </ul>
</header>

<div id="offices">
  <% @offices.each do |office| %>
    <h2><%= link_to office.name, office %></h2>
    <p><%= office.description %></p>
  <% end %>
</div>